{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.0.0"
      }
    ],
    [
      "@babel/preset-react",
      {
        "pragma": "fw.dom", // default pragma is React.createElement
        "pragmaFrag": "", // default is React.Fragment
        "throwIfNamespace": false // defaults to true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ]
}